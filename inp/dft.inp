$SP  
  FUNCTIONAL B3LYP
  UNRESTRICTED TRUE
  SCF_ALGORITHM rca_diis
  SCF_MAX_CYCLES 150
  BASIS LANL2DZ              #including ECP option
  WAVEFUNCTION_ANALYSIS FALSE

  scf_convergence 6

  SYM_IGNORE TRUE
  SYMMETRY   FALSE
$SP end

$OPT
  FUNCTIONAL B3LYP
  UNRESTRICTED TRUE
  SCF_ALGORITHM rca_diis
  SCF_MAX_CYCLES 150
  BASIS LANL2DZ
  WAVEFUNCTION_ANALYSIS FALSE
  GEOM_OPT_MAX_CYCLES 300
  GEOM_OPT_TOL_DISPLACEMENT 2500
  GEOM_OPT_TOL_GRADIENT     800
  GEOM_OPT_TOL_ENERGY      5000

  scf_convergence 6

  SYM_IGNORE TRUE
  SYMMETRY   FALSE
$OPT end

